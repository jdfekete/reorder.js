{
   "name": "reorder.js",
   "version": "2.2.5",
   "description": "Matrix reordering in JavaScript.",
   "keywords": [
      "es6",
      "visualization",
      "reordering",
      "matrix",
      "adjacency matrix",
      "parallel coordinates"
   ],
   "homepage": "https://github.com/jdfekete/reorder.js",
   "repository": {
      "type": "git",
      "url": "http://github.com/jdfekete/reorder.js.git"
   },
   "unpkg": "dist/reorder.min.js",
   "jsdelivr": "dist/reorder.min.js",
   "main": "dist/reorder.cjs.js",
   "module": "dist/reorder.esm.js",
   "browser": "dist/reorder.min.js",
   "types": "./reorder.d.ts",
   "author": [
      {
         "name": "Jean-Daniel Fekete",
         "url": "http://www.aviz.fr/~fekete"
      }
   ],
   "scripts": {
      "build": "rollup -c",
      "dev": "rollup -c rollup.config.dev.js -w",
      "pretest": "npm run build",
      "test": "vows test/*.js --nocolor; echo",
      "cover": "nyc --silent vows --nocolor",
      "cover:report": "nyc report --reporter=lcov --reporter=text",
      "clean": "rm -rf node_modules dist",
      "prettier": "prettier --write '{src,test}/**/*.js'",
      "docs": "jsdoc -c jsdoc.config.json",
      "check": "jshint src/*.js test/*.js",
      "release": "release-it"
   },
   "files": [
      "dist",
      "reorder.d.ts"
   ],
   "dependencies": {
      "@sgratzl/science": "^2.0.0"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^19.0.0",
      "@rollup/plugin-node-resolve": "^13.0.0",
      "jsdoc": "^4.0.0",
      "jshint": "^2.12.0",
      "jsonfile": "6.1.0",
      "nyc": "^15.1.0",
      "parse-path": ">=5.0.0",
      "prettier": "^2.3.0",
      "release-it": "^15.1.1",
      "rollup": "^2.47.0",
      "rollup-plugin-livereload": "^2.0.0",
      "rollup-plugin-local-resolve": "^1.0.7",
      "rollup-plugin-serve": "^1.1.0",
      "rollup-plugin-terser": "^7.0.2",
      "rollup-plugin-version-injector": "^1.3.3",
      "seedrandom": "3.0.5",
      "underscore": ">=1.13.1",
      "vows": "^0.8.3"
   },
   "license": "BSD-2-Clause",
   "jshintConfig": {
      "esversion": 6,
      "node": true,
      "laxbreak": true,
      "varstmt": true,
      "undef": true,
      "unused": true,
      "globals": {
         "console": false
      }
   },
   "prettier": {
      "singleQuote": true
   }
}
